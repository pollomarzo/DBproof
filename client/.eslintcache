Value=\"\"\n            >\n              {profiles.map((profile) => (\n                <MenuItem\n                  key={`${profile.codice_account}_${profile.numero}`}\n                  value={{\n                    codice_account: profile.codice_account,\n                    numero: profile.numero,\n                  }}\n                >\n                  {profile.nickname}\n                </MenuItem>\n              ))}\n            </Select>\n          </div>\n          <div className={classes.select}>\n            <Select\n              variant=\"filled\"\n              label=\"Contenuto\"\n              name=\"codice_contenuto\"\n              control={control}\n              defaultValue=\"\"\n            >\n              {items.map((item) => (\n                <MenuItem key={item.codice} value={item.codice}>\n                  {item.nome}\n                </MenuItem>\n              ))}\n            </Select>\n          </div>\n          <Button type=\"submit\">Confirm</Button>\n        </form>\n      </Paper>\n      <Paper className={classes.paper}>\n        <Typography>Guarda</Typography>\n        <code>{JSON.stringify(datino, null, 2)}</code>\n      </Paper>\n    </div>\n  );\n};\n\nexport default Watch;\n","/home/paolo/Documents/scuola/database/app/client/src/screens/Playlist.jsx",["55","56","57","58","59"],"/home/paolo/Documents/scuola/database/app/client/src/screens/Item.jsx",[],"/home/paolo/Documents/scuola/database/app/client/src/screens/Cast.jsx",[],"/home/paolo/Documents/scuola/database/app/client/src/screens/Account.jsx",[],"/home/paolo/Documents/scuola/database/app/client/src/constants.js",[],"/home/paolo/Documents/scuola/database/app/client/src/Select.jsx",[],"/home/paolo/Documents/scuola/database/app/client/src/screens/Feedback.jsx",[],{"ruleId":"60","replacedBy":"61"},{"ruleId":"62","replacedBy":"63"},{"ruleId":"64","severity":1,"message":"65","line":43,"column":17,"nodeType":"66","messageId":"67","endLine":43,"endColumn":26},{"ruleId":"64","